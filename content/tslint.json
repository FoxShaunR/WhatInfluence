{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": [true, "never"],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-template",
      "avoid-escape"
    ]
  },
  "rulesDirectory": []
}
